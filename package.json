{
  "name": "@fxhxyz/prettier-config",
  "version": "2.0.1",
  "type": "module",
  "description": "My professional prettier config",
  "main": "./.prettierrc.json",
  "prettier": "./.prettierrc.json",
  "keywords": [
    "typescript",
    "prettier",
    "javascript",
    "prettier config",
    "config"
  ],
  "files": [
    ".prettierignore",
    ".prettierrc.cjs",
    ".prettierrc.js",
    ".prettierrc.json",
    ".editorconfig",
    "rollup.config.js",
    ".gitignore",
    ".npmignore"
  ],
  "exports": {
    ".": {
      "require": "./.prettierrc.cjs",
      "import": "./.prettierrc.js",
      "default": "./.prettierrc.js"
    }
  },
  "author": {
    "name": "fxhxyz4",
    "email": "fxhsec@proton.me",
    "url": "https://fxhxyz.vercel.app/"
  },
  "homepage": "https://github.com/fxhxyz4/prettier-config",
  "bugs": "https://github.com/fxhxyz4/prettier-config/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/fxhxyz4/prettier-config.git"
  },
  "maintainers": [
    {
      "name": "fxhxyz4",
      "email": "fxhsec@proton.me"
    }
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "prettier": "^3.x.x",
    "rollup": "^4.x.x",
    "@ianvs/prettier-plugin-sort-imports": "^x.x.x",
    "prettier-plugin-multiline-arrays": "^x.x.x"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  }
}
